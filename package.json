{
    "name": "qnode-beans",
    "version": "2.0.1",
    "description": "node.js native IoC",
    "main": "dist/index.js",
    "scripts": {
        "clean": "node_modules/.bin/gulp clean",
        "dist": "npm run clean && node_modules/.bin/gulp default",
        "cover": "npm run dist && NODE_ENV=test QNODE_PRODUCT=qnode QNODE_MODULE=beans QNODE_LOG_LEVEL=error node ./node_modules/istanbul/lib/cli.js cover -color ./node_modules/jasmine/bin/jasmine.js",
        "test": "npm run dist && NODE_ENV=test QNODE_PRODUCT=qnode QNODE_MODULE=beans QNODE_LOG_LEVEL=error node ./node_modules/jasmine/bin/jasmine.js",
        "lint": "./node_modules/.bin/eslint -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/",
        "watch": "./node_modules/.bin/esw --watch -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/"
    },
    "repository": "git@github.com:qiangyt/qnode-beans.git",
    "author": "Qiang Yiting <qiangyt@wxcount.com> (https://github.com/qiangyt)",
    "keywords": [
        "DI",
        "Dependency Injection",
        "IoC",
        "Inversion of Control",
        "node.js"
    ],
    "bugs": {
        "url": "https://github.com/qiangyt/qnode-beans/issues"
    },
    "engines": {
        "node": ">=8"
    },
    "private": false,
    "license": "MIT",
    "dependencies": {
        "@types/bunyan": "1.8.4",
        "@types/get-caller-file": "1.0.0",
        "@types/js-yaml": "3.11.2",
        "@types/lodash": "4.14.114",
        "@types/merge2": "1.1.4",
        "@types/mkdirp": "0.5.2",
        "@types/node": "8.10.0",
        "@types/rimraf": "2.0.2",
        "@types/sprintf-js": "1.1.0",
        "@types/uuid": "3.4.3",
        "bunyan": "1.8.12",
        "get-caller-file": "1.0.3",
        "gulp": "3.9.1",
        "gulp-sourcemaps": "2.6.4",
        "gulp-typescript": "5.0.0-alpha.3",
        "gulp-util": "3.0.8",
        "istanbul": "1.1.0-alpha.1",
        "jasmine": "3.1.0",
        "js-yaml": "3.12.0",
        "lodash": "4.17.10",
        "merge2": "1.2.2",
        "mkdirp": "0.5.1",
        "mock-fs": "4.5.0",
        "mock-require": "3.0.2",
        "moment": "2.22.2",
        "rimraf": "2.6.2",
        "sinon": "6.1.4",
        "sprintf-js": "1.1.1",
        "typescript": "2.9.2",
        "uuid": "3.3.2"
    },
